# Configuration File
# Generated by AnsiGo Template Module

server_name = {{ server_name }}
port = {{ port }}
environment = {{ environment }}

# Database settings
db_host = {{ db_host }}
db_port = {{ db_port }}
db_name = {{ db_name }}

# Features
{% if enable_cache %}
cache_enabled = true
cache_ttl = {{ cache_ttl | default(3600) }}
{% else %}
cache_enabled = false
{% endif %}

# Hosts
{% for host in hosts %}
host_{{ loop.index0 }} = {{ host }}
{% endfor %}
